{
  "name": "lumina-backend",
  "version": "1.0.0",
  "description": "Lumina Backend API",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "migrate:make": "knex migrate:make --knexfile knexfile.ts",
    "migrate:latest": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile knexfile.ts",
    "migrate:list": "knex migrate:list --knexfile knexfile.ts",
    "knex": "knex --knexfile knexfile.ts"
  },
  "dependencies": {
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "redis": "^4.6.10",
    "knex": "^3.0.0",
    "sqlite3": "^5.1.6",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "googleapis": "^128.0.0",
    "axios": "^1.6.0",
    "xml2js": "^0.6.2",
    "@types/xml2js": "^0.4.14",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "@types/multer": "^1.4.11",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/node": "^20.5.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/redis": "^4.0.11"
  }
}
